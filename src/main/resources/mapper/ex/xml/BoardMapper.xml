<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.bzsys.study.mapper.BoardMapper">

  <select id="selectBoard" parameterType="kr.co.bzsys.study.dto.Board" resultType="kr.co.bzsys.study.dto.Board">
    SELECT SEQ,
           TITLE,
           WRITER,
           CONTENT,
           REG_DATE,
           CNT
    FROM BOARD
